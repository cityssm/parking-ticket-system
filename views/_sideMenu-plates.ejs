<div class="column is-3 is-hidden-print" id="is-sidemenu-expanded">
  <aside class="menu">
    <p class="menu-label">
      Licence Plates
    </p>
    <ul class="menu-list">
      <li>
        <a class="<%= (headTitle === "Licence Plates" ? "is-active" : "") %>" href="/plates">
          <span class="icon">
            <i class="fas fa-fw fa-car" aria-hidden="true"></i>
          </span>
          <span>Licence Plate Search</span>
        </a>
      </li>
    </ul>

    <% if (configFns.getProperty("application.feature_mtoExportImport") && (user.userProperties.canUpdate || user.userProperties.isOperator)) { %>
      <p class="menu-label">
        MTO Integration
      </p>
      <ul class="menu-list">
        <li>
          <a class="<%= (headTitle === "MTO Licence Plate Export" ? "is-active" : "") %>" href="/plates/mtoExport">
            <span class="icon">
              <i class="fas fa-fw fa-file-export" aria-hidden="true"></i>
            </span>
            <span>Export Licence Plates</span>
          </a>
        </li>
        <li>
          <a class="<%= (headTitle === "MTO Licence Plate Ownership Import" ? "is-active" : "") %>" href="/plates/mtoImport">
            <span class="icon">
              <i class="fas fa-fw fa-file-import" aria-hidden="true"></i>
            </span>
            <span>Import Licence Plate Ownership</span>
          </a>
        </li>
      </ul>
    <% } %>
  </aside>
  <button class="button is-light is-fullwidth has-margin-top-20" id="is-sidemenu-collapse-button">
    <span class="icon is-small"><i class="fas fa-angle-double-left" aria-hidden="true"></i></span>
    <span>Hide Side Menu</span>
  </button>
</div>
<div class="column is-narrow is-hidden-print is-hidden" id="is-sidemenu-collapsed">
  <button class="button is-light has-tooltip-right" id="is-sidemenu-expand-button" data-tooltip="Expand Side Menu">
    <i class="fas fa-angle-double-right" aria-hidden="true"></i>
    <span class="sr-only">Expand Side Menu</span>
  </button>
</div>
